<h1>Projects</h1>
<ul>
  <% @projects.each do |project| %>
    <li><%= project.name -%></li>
  <% end %>
</ul>
<p>Find me in app/views/projects/index.html.erb</p>

<div class="row">
	<div class=".col-md-3 .col-md-offset-3 pull-right">
		<%= link_to "Add Project", new_project_path if current_employee.admin? -%>
	</div>
	<div class="col-md-6 col-md-offset-3">
		<ul>
			<% @projects.each do |project| %>
			<li><%= project.name -%></li>
			<% end %>
		</ul>	
	</div>
</div>
