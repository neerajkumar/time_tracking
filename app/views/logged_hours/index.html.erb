<h1>Listing logged_hours</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logged_hours.each do |logged_hour| %>
      <tr>
        <td><%= link_to 'Show', logged_hour %></td>
        <td><%= link_to 'Edit', edit_logged_hour_path(logged_hour) %></td>
        <td><%= link_to 'Destroy', logged_hour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Logged hour', new_logged_hour_path %>
