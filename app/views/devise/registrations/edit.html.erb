<div class="row">
  <div class="col-xs-12">
  <div class="col-md-12 col-md-offset-1">


<h2>Edit <%= resource_name.to_s.humanize %></h2>

<%= form_for(resource, as: resource_name, url: employee_registration_path(resource_name), html: { class: "form-horizontal", method: :put }) do |f| %>
  <%= render template: "devise/registrations/new", locals: {f: f, button_text: "Update Member"} -%>
<% end %>
  </div>
  <div class="col-xs-12">
<div>
<h3>Cancel my account</h3>

<p>Unhappy? <%= button_to "Cancel my account", employee_registration_path(resource_name), data: { confirm: "Are you sure?" }, method: :delete, class: "btn btn-default" %></p>

<%= link_to "Back", :back %>
</div>
  </div>
  </div>
  </div>
